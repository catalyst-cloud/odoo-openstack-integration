<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <report
                id="openstack_invoice_csv"
                model="account.move"
                string="OpenStack CSV"
                report_type="csv"
                name="openstack_integration.openstack_invoice_csv"
